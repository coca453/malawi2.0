$(window).scroll(function(){
	var obj_top = $(document).scrollTop();
	if(obj_top > 10){
		$('#header').height(82);
		$('#header .contenido .tresovejas').width(55);
		$('#header .contenido .tresovejas').css('margin-top','10px');
		$('#header .inner').addClass('header-scroll');
		$('#header .inner').css('padding-bottom','0');
		$('#header .base').addClass('base-scroll');
		$('#header .contenido .opciones').css('margin-top','22px');
	}else{
		$('#header').height(153);
		$('#header .contenido .tresovejas').width(112);
		$('#header .contenido .tresovejas').css('margin-top','24px');
		$('#header .inner').removeClass('header-scroll');
		$('#header .inner').css('padding-bottom','10px');
		$('#header .base').removeClass('base-scroll');
		$('#header .contenido .opciones').css('margin-top','40px');
	}
});
function abreMenu(){
	var estatus = $('#menu').css('display');
	if(estatus == 'none'){
		$('#menu').fadeIn();
		var menuHeight = $('.blanco .contenido').height();
		$('.verde .contenido').height(menuHeight);
	}else{
		$('#menu').fadeOut();
	}
}
function cierraMenu(){
	$('#menu').fadeOut();
}

function scrollScreen(val){
	var valor = $('#'+val).offset().top;
	var broserWidth = $(window).width();
	if(broserWidth > 767){
		$('html,body').animate({scrollTop: (valor - 125)}, 'slow');
	}else{
		$('html,body').animate({scrollTop: (valor - 85)}, 'slow');
	}
}

