function seleccionaLugar(tipo,id,orden,relacionado){
	$('#seleccion_listado_1').removeClass('seleccionado');
	if(tipo == 'frente'){
		var estatus_zona = $('.frente').css('display');
		if(estatus_zona == 'none'){
			$('.dorso').fadeOut(0);
			$('.frente').fadeIn(0);
			$('.thumb-frente').fadeOut();
			$('.thumb-dorso').fadeIn();
		}
	}
	if(tipo == 'dorso'){
		var estatus_zona = $('.dorso').css('display');
		if(estatus_zona == 'none'){
			$('.frente').fadeOut(0);
			$('.dorso').fadeIn(0);
			$('.thumb-dorso').fadeOut();
			$('.thumb-frente').fadeIn();
		}
	}

	$('.mask-vacia').css('opacity',0);

	$('#mask-'+id).css('opacity',1);
	$('#mask-'+id+' img').attr('src','uploads/trama.php?img1='+id+'-reverso.png&img2=trama-seleccionado.png');
	if(relacionado != ''){
		$('#mask-'+relacionado).css('opacity',1);
		$('#mask-'+relacionado+' img').attr('src','uploads/trama.php?img1='+id+'-reverso.png&img2=trama-seleccionado.png');
	}

	$('#seleccion').attr('value',id);
	$('#relacionado').attr('value',relacionado);
	$('.lista-opcion').removeClass('seleccionado');
	$('.lista-titulo').removeClass('seleccionado');
	$('#seleccion_listado_'+orden).addClass('seleccionado');
	$('#paso2-color').fadeIn();

	var broserWidth = $(window).width();
	if(broserWidth < 768){
		scrollScreen('paso2-color');
		despliegaPartes();
	}
}
